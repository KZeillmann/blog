---
// Component Imports
import BaseHead from '../components/BaseHead.astro';
import '../styles/synth-demo.css';
import WebApiPlayground from '../components/web-api-playground/WebApiPlayground';
// Component Script:
// You can write any JavaScript/TypeScript that you'd like here.
// It will run during the build, but never in the browser.
// All variables are available to use in the HTML template below.
const title = 'Synth Demo';
const description = 'A demo for the synth project';
const permalink = 'https://kzeillmann.com/';



// Data Fetching: List all Markdown posts in the repo.
---
<html lang="en">
	<head>
		<BaseHead {title} {description} {permalink} />
    </head>
    <body>
        <WebApiPlayground client:only />
    </body>
</html>